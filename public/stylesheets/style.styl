//Header

#header {
  position: fixed;
  display: block;
  width: 100%;
  height: 60px;
  z-index: 9;
  min-width: 320px;
  .main {
    position: relative;
    z-index: 3;
  }
  .after {
    position: absolute;
    width: 100%;
    height: 100%;
    top: -60px;
    z-index: 2;
    background-color: #fff;
    -webkit-box-shadow: 0px 0px 65px 0px rgba(0,0,0,0.4);
    -moz-box-shadow: 0px 0px 65px 0px rgba(0,0,0,0.4);
    box-shadow: 0px 0px 65px 0px rgba(0,0,0,0.4);
  }
  &.sticky {
    .after {
      top: 0;
      -webkit-box-shadow: 0px 5px 0px 0px rgba(0,0,0,0.05);
      -moz-box-shadow: 0px 5px 0px 0px rgba(0,0,0,0.05);
      box-shadow: 0px 5px 0px 0px rgba(0,0,0,0.05);
    }
    #navigation {
      li {
        a {
          color: #000;
        }
        span {
          &.border {
            background-color: #ef6661;
          }
        }
      }
      ul {
        li {
          a {
            color: #fff;
          }
        }
      }
    }
    #logo {
      .label {
        background-color: #000;
        transform: rotate(0deg);
        -moz-transform: rotate(0deg);
        -webkit-transform: rotate(0deg);
      }
      h1 {
        color: #fff;
      }
    }
  }
}

