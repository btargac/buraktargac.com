  }
  .inner {
    display: none;
  }
  .spinner {
    position: absolute;
    left: 50%;
    margin-left: -15px;
  }
}

#portfolio {
  float: left;
  display: block;
  width: 100%;
  position: relative;
  background-color: #f9f9f9;
  padding-bottom: 30px;
  .inner {
    background-color: #000;
  }
  li {
    float: left;
    display: block;
    width: 25%;
    background-color: #000;
    a {
      float: left;
      display: block;
    }
    &:nth-child(4n {
      & + 1) {
        .item {
          z-index: 4;
        }
      }
      & + 2) {
        .item {
          z-index: 3;
        }
      }
      & + 3) {
        .item {
          z-index: 2;
        }
      }
      & + 4) {
        .item {
          z-index: 1;
        }
      }
    }
    &.hidden {
      display: none;
    }
  }
  .item {
    float: left;
    left: 0;
    width: 100%;
    position: relative;
    img {
      float: left;
      width: 100%;
      height: 100%;
      &.client {
        float: none;
        display: inline-block;
        width: auto;
        max-width: 200px;
        height: auto;
      }
    }
    .over {
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background-color: rgba(0,0,0,0.6);
      text-align: center;
      h2 {
        color: #fff;
        margin-top: 30%;
        font-weight: 300;
        padding: 15px;
        font-size: 23px;
        width: auto;
      }
      .border {
        height: 1px;
        background-color: #fff;
      }
      h3 {
        color: #fff;
        font-size: 18px;
        font-weight: 300;
        padding: 13px 50px;
        box-sizing: border-box;
        width: auto;
      }
    }
    &.aberto {
      .over {
        display: none;
      }
    }
    &:hover {
      & > .over {
        opacity: 0;
      }
    }
    &.ajax {
      opacity: 0;
      left: -150px;
    }
  }
  .more {
    position: absolute;
    left: 50%;
    bottom: 10px;
    width: 40px;
    height: 40px;
    margin-left: -20px;
    background-color: #ef6661;
