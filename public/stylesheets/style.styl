//scroll button animation
@keyframes arrows {
  0% { bottom:40px; }
  10% { bottom:50px; }
  20% { bottom:40px; }
  30% { bottom:50px; }
  40% { bottom:30px; }
  50% { bottom:50px; }
  60% { bottom:40px; }
  70% { bottom:50px; }
  80% { bottom:30px; }
  90% { bottom:50px; }
  100% { bottom:40px; }
}
@-webkit-keyframes arrows {
  0% { bottom:40px; }
  10% { bottom:50px; }
  20% { bottom:40px; }
  30% { bottom:50px; }
  40% { bottom:30px; }
  50% { bottom:50px; }
  60% { bottom:40px; }
  70% { bottom:50px; }
  80% { bottom:30px; }
  90% { bottom:50px; }
  100% { bottom:40px; }
}

.scroll-btn {
  -webkit-animation: arrows 3.8s infinite;
  -webkit-animation-delay: 1s;
  animation: arrows 3.8s infinite;
  animation-delay: 1s;
  cursor: pointer;
}



