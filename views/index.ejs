		<!-- carousel -->
		<section id="carousel">

			<span class="pattern"></span>
			
			<div class="main-text">Welcome to my page</div>
			<div class="sub-text">
				<ul class="slide-text">
					<li>I’m a front-end developer with a passion for the web.</li>
					<li>Currently living & loving in Istanbul, Turkey.</li>
				</ul>
			</div>
		
			<div id="wrapper-slider">
				<div id="controls">
					<div class="prev"></div>
					<div class="next"></div>
				</div>
			</div>
			
			<a href="index.html#portfolio" class="scroll-btn anchor"></a>	
			
		</section>
		
		<div id="portfolio-box">
			<div class="spinner"></div>
			<div class="inner"></div>
		</div>
		
		<section id="portfolio">
			<div class="inner">
				<ul id="rb-grid" class="list rb-grid">
					
					<% portfolios.forEach(function(portfolio){ %>
					<li>
						<a class="item" href="<%= portfolio.detailPageUrl %>">
							<img src="<%= portfolio.imgUrl %>" alt="">
							<div class="over">
								<h2 class="page-title"><%= portfolio.company %><span class="border"></span></h2>
								<h3 class="page-subtitle">
									<%= portfolio.definition %>
								</h3>
							</div>
						</a>
					</li>

					<% }); %>
					
				
				</ul>
				
				<a href="#" class="more rotate"><span class="rotate">+</span></a>
			</div>
		</section>
		
